	<subDataset name="PieChartDataset" uuid="f3f476a7-3651-4b88-80dd-8874dc8fd028"/>
	<queryString language="SQL">
		<![CDATA[SELECT
     equipo."nombre" AS "Equipo",
     jugador."nombre" AS "Nombre",
     jugador."apellido" AS "Apellido",
     sancion."nombre" AS "Sanción",
     sancion."descripcion" AS "Descripción",
     jugador_equipo_sancion."fecha_inicio" AS "Fecha_Inicio",
     jugador_equipo_sancion."fecha_fin" AS "Fecha_Fin"
FROM
     "public"."sancion" sancion INNER JOIN "public"."jugador_equipo_sancion" jugador_equipo_sancion ON sancion."id_sancion" = jugador_equipo_sancion."id_sancion"
     INNER JOIN "public"."jugador_equipo" jugador_equipo ON jugador_equipo_sancion."id_jugador" = jugador_equipo."id_jugador"
     AND jugador_equipo."id_equipo" = jugador_equipo_sancion."id_equipo"
     INNER JOIN "public"."jugador" jugador ON jugador_equipo."id_jugador" = jugador."id_jugador"
     INNER JOIN "public"."equipo" equipo ON jugador_equipo."id_equipo" = equipo."id_equipo"]]>
	</queryString>
